{
  "name": "hs-standard-sets",
  "version": "1.0.0",
  "description": "Utilities for the Hearthstone standard card set.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "$(npm bin)/rollup -c",
    "build": "$(npm bin)/rollup -c",
    "test": "NODE_ENV=test $(npm bin)/mocha --require @babel/register --recursive test/*-test.js test/**/*-test.js",
    "lint": "$(npm bin)/eslint src/*.js src/**/*.js",
    "lint:fix": "$(npm bin)/eslint src/*.js src/**/*.js --fix",
    "flow": "$(npm bin)/flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakai-akinobu/hs-standard-sets.git"
  },
  "keywords": [
    "hearthstone"
  ],
  "author": "sakai-akinobu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sakai-akinobu/hs-standard-sets/issues"
  },
  "homepage": "https://github.com/sakai-akinobu/hs-standard-sets#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "flow-bin": "^0.80.0",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^2.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "lodash": "^4.17.10"
  }
}
